{
  "name": "atacut",
  "version": "1.0.0",
  "description": "ATACUT - Professional Video Editor. Create, edit, and export videos with advanced color grading, effects, and text overlays.",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron dist/main.js",
    "build": "webpack --config webpack.main.config.js && webpack --config webpack.renderer.config.js && node -e \"require('fs').copyFileSync('package.json', 'dist/package.json');\" && npm run copy-ffmpeg",
    "build-and-start": "npm run build && npm start",
    "copy-ffmpeg": "node scripts/copy-ffmpeg.js",
    "package": "npm run build && electron-builder",
    "package-win": "npm run build && electron-builder --win",
    "package-linux": "npm run build && electron-builder --linux",
    "dev": "concurrently \"webpack --watch --config webpack.main.config.js\" \"webpack --watch --config webpack.renderer.config.js\"",
    "test": "jest"
  },
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "immer": "^10.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hotkeys-hook": "^5.2.1",
    "react-virtualized-auto-sizer": "^1.0.20",
    "react-window": "^1.8.10",
    "uuid": "^9.0.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^9.0.7",
    "babel-loader": "^9.1.0",
    "concurrently": "^8.2.0",
    "css-loader": "^6.8.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.13.3",
    "electron-packager": "^17.1.2",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.4",
    "png-to-ico": "^3.0.1",
    "style-loader": "^3.3.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.3.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.0"
  },
  "keywords": [
    "video",
    "editor",
    "electron",
    "react",
    "typescript",
    "4k",
    "ffmpeg",
    "content-creator"
  ],
  "author": "Your Name",
  "license": "MIT"
}
